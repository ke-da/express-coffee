@import 'lib/bootstrap/bootstrap.styl' // bootstrap-stylus
@import 'nib'
.container
  margin-top 30px

h2
  smooth-button()
  width: 90%
  font-size: 2em

h3
  margin-top: 20px
  text-align: center

pre
  font-family: courier
  background-color: rgba(0,0,0,.8)
  color: yellow
  width: 100%
  margin-bottom: 20px
  padding: 10px
  border: 1px solid black

.head
  clear: both
  padding-top: 30px

.tile
  display: block
  float: left
  color: #fff
  padding: 19px
  min-height: 20px
  margin-left: 20px

.tile:hover
  margin-top: -2px
  text-decoration: none
  color: #fff

.tile.first
  margin-left: 0px

.footer
  padding:20px

.content
  text-align: center
  max-width: 250px